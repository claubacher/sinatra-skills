<div class="container">
  <h1>User Skills and Levels</h1>
  <table>
  	<tr>
  	  <th>Name</th>
  	  <th>Skill</th>
  	  <th>Years</th>
	  <th>Formal?</th>
	</tr>
  	<% @users.each do |user| %>
	  	<tr>
	  	  <td rowspan="<%= user.proficiencies.count %>"><%= user.name %></td>
	  	<% user.proficiencies.each do |proficiency| %>
		  <td><%= proficiency.skill.name %></td>
		  <td><%= proficiency.years %></td>
		  <td><%= proficiency.formal %></td>
		  </tr>
		  <tr>
	  	<% end %>
		</tr>
  	<% end %>
  </table>
  <% if session[:user_id] %>
  	<%= erb :_add_skills %>
  <% end %>
</div>
